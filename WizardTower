public class WizardTower extends Building{
 
  //setting the buildings location and dimensions
  public WizardTower(float x, float y, float w, float h){
    this.x = x;
    this.y = y;
    this.w = w;
    this.h = h;
    health = 20;
    fireType = new WizardFire(0,0,0,0,0);
    img = loadImage("Tower.png");
  }
  
}
